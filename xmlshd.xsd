						<xsd:documentation> \en thumbnail image \enden  \ja サムネールイメージ \endja </xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="aux">
					<xsd:annotation>
						<xsd:documentation> \en aux. flags \enden  \ja その他フラグ \endja </xsd:documentation>
					</xsd:annotation>
					<xsd:complexType>
						<xsd:attribute name="white_key_mask">
							<xsd:annotation>
								<xsd:documentation> \en white key mask \enden  \ja ホワイトキーマスク \endja </xsd:documentation>
							</xsd:annotation>
						</xsd:attribute>
						<xsd:attribute name="black_key_mask">
							<xsd:annotation>
								<xsd:documentation> \en black key mask \enden  \ja ブラックキーマスク \endja </xsd:documentation>
							</xsd:annotation>
						</xsd:attribute>
