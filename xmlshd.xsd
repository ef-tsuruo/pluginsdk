															<xsd:attribute name="backdrop">
																<xsd:annotation>
																	<xsd:documentation> \en backdrop \enden  \ja バックドロップを合成する \endja </xsd:documentation>
																</xsd:annotation>
															</xsd:attribute>
														</xsd:complexType>
