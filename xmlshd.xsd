													</xsd:element>
													<xsd:element ref="attribute">
														<xsd:annotation>
															<xsd:documentation> \en attribute \enden  \ja renderersに含まれる個々のattribute \endja </xsd:documentation>
														</xsd:annotation>
													</xsd:element>
												</xsd:choice>
											</xsd:complexType>
										</xsd:element>
										<xsd:element name="effectors">
											<xsd:annotation>
												<xsd:documentation> \en array of effectors \enden  \ja レンダリングエフェクターの配列 \endja </xsd:documentation>
											</xsd:annotation>
											<xsd:complexType>
												<xsd:choice minOccurs="0" maxOccurs="unbounded">
