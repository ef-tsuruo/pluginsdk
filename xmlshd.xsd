						</xsd:choice>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="template_images">
					<xsd:annotation>
						<xsd:documentation> \en array of template images \enden  \ja テンプレートイメージの配列 \endja </xsd:documentation>
					</xsd:annotation>
					<xsd:complexType>
						<xsd:choice minOccurs="0" maxOccurs="unbounded">
							<xsd:element name="opacity">
								<xsd:annotation>
									<xsd:documentation> \en opacity \enden  \ja テンプレートイメージ表示の際の不透明度 \endja </xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="pers_view">
								<xsd:annotation>
									<xsd:documentation> \en pers_view \enden  \ja 透視図に表示するテンプレートイメージ \endja </xsd:documentation>
									<xsd:documentation> \en pers_view \enden  \ja -1 : 透視図に読み込んだイメージをそのまま表示 \endja </xsd:documentation>
									<xsd:documentation> \en pers_view \enden  \ja 0 : 側面図に読み込んだイメージをZ-Y平面上に表示 \endja </xsd:documentation>
									<xsd:documentation> \en pers_view \enden  \ja 1 : 上面図に読み込んだイメージをX-Z平面上に表示 \endja </xsd:documentation>
									<xsd:documentation> \en pers_view \enden  \ja 2 : 正面図に読み込んだイメージをX-Y平面上に表示 \endja </xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="visible">
								<xsd:annotation>
									<xsd:documentation> \en visible \enden  \ja 表示 \endja </xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="template_image" maxOccurs="unbounded">
								<xsd:annotation>
									<xsd:documentation> \en template image \enden  \ja 個々のテンプレートイメージ \endja </xsd:documentation>
								</xsd:annotation>
								<xsd:complexType>
									<xsd:choice minOccurs="0" maxOccurs="unbounded">
										<xsd:element name="position">
											<xsd:annotation>
												<xsd:documentation> \en position \enden  \ja 位置 \endja </xsd:documentation>
											</xsd:annotation>
										</xsd:element>
										<xsd:element name="size">
											<xsd:annotation>
												<xsd:documentation> \en size \enden  \ja サイズ \endja </xsd:documentation>
											</xsd:annotation>
										</xsd:element>
										<xsd:element ref="pixels" minOccurs="0">
											<xsd:annotation>
												<xsd:documentation> \en actual image data \enden  \ja イメージデータの実体 \endja </xsd:documentation>
											</xsd:annotation>
										</xsd:element>
									</xsd:choice>
								</xsd:complexType>
							</xsd:element>
						</xsd:choice>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="scene_info">
					<xsd:annotation>
						<xsd:documentation> \en scene info \enden  \ja シーンの情報 \endja </xsd:documentation>
					</xsd:annotation>
					<xsd:complexType>
						<xsd:choice minOccurs="0" maxOccurs="unbounded">
							<xsd:element name="divider">
								<xsd:annotation>
									<xsd:documentation> \en divider position \enden  \ja 図面分割ラインの位置 \endja </xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="spos">
								<xsd:annotation>
									<xsd:documentation> \en spos \enden  \ja spos \endja </xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="center">
								<xsd:annotation>
									<xsd:documentation> \en center position \enden  \ja ３面図表示の中心位置 \endja </xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element ref="view_info">
								<xsd:annotation>
									<xsd:documentation> \en view_info \enden  \ja view_info \endja </xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="current_frame">
								<xsd:annotation>
									<xsd:documentation> \en current frame \enden  \ja 現行のフレーム値 \endja </xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="frame_scale">
								<xsd:annotation>
									<xsd:documentation> \en frame scale \enden  \ja フレームスケール値 \endja </xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="elapsed_time">
								<xsd:annotation>
									<xsd:documentation> \en elapsed time \enden  \ja レンダリングにかかった時間 \endja </xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="show_normals">
								<xsd:annotation>
									<xsd:documentation> \en show normal \enden  \ja 法線を表示 \endja </xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="default_bevel_radius">
								<xsd:annotation>
									<xsd:documentation> \en default bevel size \enden  \ja ベベルサイズのデフォルト値 \endja </xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="default_disk_radius">
								<xsd:annotation>
									<xsd:documentation> \en default disk radius \enden  \ja 円形状の半径のデフォルト値 \endja </xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="default_sphere_radius">
								<xsd:annotation>
									<xsd:documentation> \en default sphere radius \enden  \ja 円形状の半径のデフォルト値 \endja </xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="default_light_radius">
								<xsd:annotation>
									<xsd:documentation> \en default light size \enden  \ja 光源形状のサイズのデフォルト値 \endja </xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="safe_zone_size">
								<xsd:annotation>
									<xsd:documentation> \en safe zone size \enden  \ja セーフゾーンのサイズ \endja </xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="rev16c_compatible_rotation">
								<xsd:annotation>
									<xsd:documentation> \en rev16c_compatible_rotation \enden  \ja Rev.16c互換の回転値 \endja </xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="main_frame_maximized">
								<xsd:annotation>
									<xsd:documentation> \en main_frame_maximized \enden  \ja メインフレームが最大化されている \endja </xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="main_frame">
								<xsd:annotation>
									<xsd:documentation> \en main_frame \enden  \ja メインフレーム \endja </xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="flags">
								<xsd:annotation>
									<xsd:documentation> \en flags \enden  \ja 各種フラグ \endja </xsd:documentation>
								</xsd:annotation>
								<xsd:complexType>
									<xsd:attribute name="snap">
										<xsd:annotation>
											<xsd:documentation> \en snap \enden  \ja スナップ \endja </xsd:documentation>
										</xsd:annotation>
									</xsd:attribute>
									<xsd:attribute name="ruler">
										<xsd:annotation>
											<xsd:documentation> \en ruler \enden  \ja 定規表示 \endja </xsd:documentation>
										</xsd:annotation>
									</xsd:attribute>
									<xsd:attribute name="grid">
										<xsd:annotation>
											<xsd:documentation> \en grid \enden  \ja ３面図グリッド表示 \endja </xsd:documentation>
										</xsd:annotation>
									</xsd:attribute>
									<xsd:attribute name="pers_grid">
										<xsd:annotation>
											<xsd:documentation> \en pers_grid \enden  \ja 透視図グリッド表示 \endja </xsd:documentation>
										</xsd:annotation>
									</xsd:attribute>
									<xsd:attribute name="current_frame_is_good">
										<xsd:annotation>
											<xsd:documentation> \en current_frame_is_good \enden  \ja 現行のフレーム値が有効 \endja </xsd:documentation>
										</xsd:annotation>
									</xsd:attribute>
									<xsd:attribute name="local_part">
										<xsd:annotation>
											<xsd:documentation> \en local_part \enden  \ja ローカル座標系表示の対象となっているパート \endja </xsd:documentation>
										</xsd:annotation>
									</xsd:attribute>
									<xsd:attribute name="multi_handle">
										<xsd:annotation>
											<xsd:documentation> \en multi_handle \enden  \ja 選択されている全てのハンドルが連動する \endja </xsd:documentation>
										</xsd:annotation>
									</xsd:attribute>
									<xsd:attribute name="large_point">
										<xsd:annotation>
											<xsd:documentation> \en large_point \enden  \ja ラージポイント \endja </xsd:documentation>
										</xsd:annotation>
									</xsd:attribute>
									<xsd:attribute name="no_snap_frame">
										<xsd:annotation>
											<xsd:documentation> \en no_snap_frame \enden  \ja フレーム値をスナップしない \endja </xsd:documentation>
										</xsd:annotation>
									</xsd:attribute>
									<xsd:attribute name="auto_key">
										<xsd:annotation>
											<xsd:documentation> \en auto_key \enden  \ja キーフレームを自動生成 \endja </xsd:documentation>
										</xsd:annotation>
									</xsd:attribute>
									<xsd:attribute name="no_frame_skip">
										<xsd:annotation>
											<xsd:documentation> \en no_frame_skip \enden  \ja モーションのプレビュー時に実時間再生をしない。 \endja </xsd:documentation>
										</xsd:annotation>
									</xsd:attribute>
									<xsd:attribute name="preview_loop">
										<xsd:annotation>
											<xsd:documentation> \en preview_loop \enden  \ja モーションのプレビューをループさせる。 \endja </xsd:documentation>
										</xsd:annotation>
									</xsd:attribute>
								</xsd:complexType>
							</xsd:element>
						</xsd:choice>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="views">
					<xsd:annotation>
						<xsd:documentation> \en array of view settings \enden  \ja 視野の配列 \endja </xsd:documentation>
					</xsd:annotation>
					<xsd:complexType>
						<xsd:choice minOccurs="0" maxOccurs="unbounded">
							<xsd:element name="current_metacamera">
								<xsd:annotation>
									<xsd:documentation> \en current metacamera \enden  \ja 現行のメタカメラのインデクス値 \endja </xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element ref="view">
								<xsd:annotation>
									<xsd:documentation> \en view \enden  \ja 視野情報 \endja </xsd:documentation>
								</xsd:annotation>
							</xsd:element>
						</xsd:choice>
					</xsd:complexType>
				</xsd:element>
				<xsd:element ref="part">
					<xsd:annotation>
						<xsd:documentation> \en part \enden  \ja ルートパート \endja </xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="end_of_file">
					<xsd:annotation>
						<xsd:documentation> \en end of file marker \enden  \ja ファイル終了マーカ \endja </xsd:documentation>
					</xsd:annotation>
					<xsd:complexType/>
				</xsd:element>
			</xsd:choice>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="skin">
		<xsd:annotation>
			<xsd:documentation> \en skin settings \enden  \ja スキン設定 \endja </xsd:documentation>
			<xsd:documentation> \en blah \enden  \ja 0個以上の、ジョイントと荷重の値のペアが並ぶ。 \endja </xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:choice minOccurs="0" maxOccurs="unbounded">
				<xsd:element name="index">
					<xsd:annotation>
						<xsd:documentation> \en index \enden  \ja ポリゴンメッシュ頂点インデクス値 \endja </xsd:documentation>
						<xsd:documentation> \en blah \enden  \ja ポリゴンメッシュの頂点に設定されたスキンの場合、その頂点のインデクス値。それ以外の場合には無効。 \endja </xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element ref="joint_specifier">
					<xsd:annotation>
						<xsd:documentation> \en joint \enden  \ja このジョイントから影響を受ける \endja </xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="weight">
					<xsd:annotation>
						<xsd:documentation> \en weight \enden  \ja ジョイントからの影響の荷重 \endja </xsd:documentation>
					</xsd:annotation>
				</xsd:element>
			</xsd:choice>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="solid">
		<xsd:annotation>
			<xsd:documentation> \en solid \enden  \ja ソリッド \endja </xsd:documentation>
			<xsd:documentation> \en blah \enden  \ja 回転体または掃引体の設定 \endja </xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:choice minOccurs="0" maxOccurs="unbounded">
				<xsd:element name="revolve">
					<xsd:annotation>
						<xsd:documentation> \en revole \enden  \ja 回転体の設定 \endja </xsd:documentation>
					</xsd:annotation>
					<xsd:complexType>
						<xsd:choice minOccurs="0" maxOccurs="unbounded">
							<xsd:element name="axis_matrix">
								<xsd:annotation>
									<xsd:documentation> \en transformation matrix to define revolution axis \enden  \ja 回転軸を定義する変換マトリクス \endja </xsd:documentation>
									<xsd:documentation> \en blah \enden  \ja Z軸をこのマトリクスで座標変換した結果が、回転軸となる。 \endja </xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="from">
								<xsd:annotation>
									<xsd:documentation> \en from \enden  \ja 開始角度 \endja </xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="to">
								<xsd:annotation>
									<xsd:documentation> \en to \enden  \ja 終了角度 \endja </xsd:documentation>
								</xsd:annotation>
							</xsd:element>
						</xsd:choice>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="extrude">
					<xsd:annotation>
						<xsd:documentation> \en extrude \enden  \ja 掃引体の設定 \endja </xsd:documentation>
					</xsd:annotation>
					<xsd:complexType>
						<xsd:choice minOccurs="0" maxOccurs="unbounded">
							<xsd:element name="direction">
								<xsd:annotation>
									<xsd:documentation> \en direction \enden  \ja 掃引の方向（と距離） \endja </xsd:documentation>
								</xsd:annotation>
							</xsd:element>
						</xsd:choice>
					</xsd:complexType>
				</xsd:element>
			</xsd:choice>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="surface">
		<xsd:annotation>
			<xsd:documentation> \en surface attributes \enden  \ja 表面材質設定 \endja </xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:choice minOccurs="0" maxOccurs="unbounded">
				<xsd:element name="flags">
					<xsd:annotation>
						<xsd:documentation> \en flags \enden  \ja 各種フラグ \endja </xsd:documentation>
					</xsd:annotation>
					<xsd:complexType>
						<xsd:attribute name="has_diffuse">
							<xsd:annotation>
								<xsd:documentation> \en has_diffuse \enden  \ja 拡散反射値を持っている \endja </xsd:documentation>
								<xsd:documentation> \en blah \enden  \ja 値を持っていない場合には、親階層、さらには表面材質デフォルト設定値から継承する。 \endja </xsd:documentation>
							</xsd:annotation>
						</xsd:attribute>
						<xsd:attribute name="has_specular">
							<xsd:annotation>
								<xsd:documentation> \en has_specular \enden  \ja 鏡面反射値を持っている \endja </xsd:documentation>
								<xsd:documentation> \en blah \enden  \ja 値を持っていない場合には、親階層、さらには表面材質デフォルト設定値から継承する。 \endja </xsd:documentation>
							</xsd:annotation>
						</xsd:attribute>
						<xsd:attribute name="has_specular_2">
