								</xsd:annotation>
							</xsd:element>
							<xsd:element name="mapping_type">
								<xsd:annotation>
									<xsd:documentation> \en channel \enden  \ja チャネル \endja </xsd:documentation>
									<xsd:documentation> \en blah \enden  \ja 0 : 拡散反射 \endja </xsd:documentation>
									<xsd:documentation> \en blah \enden  \ja 1 : 光沢１ \endja </xsd:documentation>
									<xsd:documentation> \en blah \enden  \ja 2 : 光沢２ \endja </xsd:documentation>
									<xsd:documentation> \en blah \enden  \ja 3 : 反射 \endja </xsd:documentation>
									<xsd:documentation> \en blah \enden  \ja 4 : 透明度 \endja </xsd:documentation>
									<xsd:documentation> \en blah \enden  \ja 5 : バンプ \endja </xsd:documentation>
									<xsd:documentation> \en blah \enden  \ja 6 : トリム \endja </xsd:documentation>
									<xsd:documentation> \en blah \enden  \ja 7 : 環境 \endja </xsd:documentation>
									<xsd:documentation> \en blah \enden  \ja 8 : 発光 \endja </xsd:documentation>
									<xsd:documentation> \en blah \enden  \ja 9 : バックライト \endja </xsd:documentation>
									<xsd:documentation> \en blah \enden  \ja 10 : 荷重 \endja </xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="pattern_type_uuid">
								<xsd:annotation>
									<xsd:documentation> \en pattern \enden  \ja パターン \endja </xsd:documentation>
									<xsd:documentation> \en pattern \enden  \ja 先頭の32ビット以外が全てゼロの場合には、先頭32ビットによって以下のように解釈する。 \endja </xsd:documentation>
									<xsd:documentation> \en blah \enden  \ja 0 : 無し \endja </xsd:documentation>
									<xsd:documentation> \en blah \enden  \ja 2 : ストライプ \endja </xsd:documentation>
									<xsd:documentation> \en blah \enden  \ja 3 : チェック \endja </xsd:documentation>
									<xsd:documentation> \en blah \enden  \ja 4 : スポット \endja </xsd:documentation>
									<xsd:documentation> \en blah \enden  \ja 5 : 大理石 \endja </xsd:documentation>
									<xsd:documentation> \en blah \enden  \ja 6 : 木目 \endja </xsd:documentation>
									<xsd:documentation> \en blah \enden  \ja 7 : 丸太 \endja </xsd:documentation>
									<xsd:documentation> \en blah \enden  \ja 10 : 波 \endja </xsd:documentation>
									<xsd:documentation> \en blah \enden  \ja 11 : 海 \endja </xsd:documentation>
									<xsd:documentation> \en blah \enden  \ja 12 : 雲 \endja </xsd:documentation>
									<xsd:documentation> \en pattern \enden  \ja それ以外の場合には、シェーダプラグインの識別子。 \endja </xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="mapping_projection_type">
								<xsd:annotation>
									<xsd:documentation> \en projection type \enden  \ja 投影の種類 \endja </xsd:documentation>
									<xsd:documentation> \en blah \enden  \ja 0 : X軸方向平行投影 \endja </xsd:documentation>
									<xsd:documentation> \en blah \enden  \ja 0 : Y軸方向平行投影 \endja </xsd:documentation>
									<xsd:documentation> \en blah \enden  \ja 0 : Z軸方向平行投影 \endja </xsd:documentation>
									<xsd:documentation> \en blah \enden  \ja 0 : ラップ／３軸方向 \endja </xsd:documentation>
									<xsd:documentation> \en blah \enden  \ja 0 : 円筒 \endja </xsd:documentation>
									<xsd:documentation> \en blah \enden  \ja 0 : 球 \endja </xsd:documentation>
								</xsd:annotation>
