												<xsd:documentation> \en shadow map size \enden  \ja シャドウマップのサイズ \endja </xsd:documentation>
											</xsd:annotation>
										</xsd:element>
										<xsd:element name="shadow_map_sampling">
											<xsd:annotation>
												<xsd:documentation> \en shadow map sampling \enden  \ja シャドウマップのサンプリング \endja </xsd:documentation>
												<xsd:documentation> \en blah \enden  \ja 0 : ４×４ ジタリングする \endja </xsd:documentation>
												<xsd:documentation> \en blah \enden  \ja 1 : ８×８ ジタリングする \endja </xsd:documentation>
												<xsd:documentation> \en blah \enden  \ja 2 : １６×１６ ジタリングしない \endja </xsd:documentation>
											</xsd:annotation>
										</xsd:element>
										<xsd:element name="minimum_samplings">
											<xsd:annotation>
												<xsd:documentation> \en minimum samplings \enden  \ja サンプリング数最小値 \endja </xsd:documentation>
