												<xsd:documentation> \en compatibility mode \enden  \ja レンダリング互換モード \endja </xsd:documentation>
												<xsd:documentation> \en blah \enden  \ja 0 : Shade R5 \endja </xsd:documentation>
												<xsd:documentation> \en blah \enden  \ja 1 : Shade 6 spirit (rev.18) \endja </xsd:documentation>
												<xsd:documentation> \en blah \enden  \ja 2 : Shade 6 (rev.19) \endja </xsd:documentation>
												<xsd:documentation> \en blah \enden  \ja 3 : Shade 6 (rev.20) \endja </xsd:documentation>
												<xsd:documentation> \en blah \enden  \ja 4 : Shade 7 basic (rev.37) \endja </xsd:documentation>
												<xsd:documentation> \en blah \enden  \ja 5 : Shade 7.1.1 \endja </xsd:documentation>
												<xsd:documentation> \en blah \enden  \ja 6 : Shade 7.5 \endja </xsd:documentation>
												<xsd:documentation> \en blah \enden  \ja 適当に大きな数 : 現行版 \endja </xsd:documentation>
											</xsd:annotation>
										</xsd:element>
										<xsd:element name="global_illumination">
											<xsd:annotation>
												<xsd:documentation> \en global illumination \enden  \ja 大域照明の種類 \endja </xsd:documentation>
												<xsd:documentation> \en blah \enden  \ja 0 : 無し \endja </xsd:documentation>
												<xsd:documentation> \en blah \enden  \ja 0 : パストレーシング \endja </xsd:documentation>
												<xsd:documentation> \en blah \enden  \ja 0 : フォトンマッピング \endja </xsd:documentation>
												<xsd:documentation> \en blah \enden  \ja 0 : ラジオシティ \endja </xsd:documentation>
											</xsd:annotation>
										</xsd:element>
										<xsd:element name="target_number_of_photons">
											<xsd:annotation>
												<xsd:documentation> \en target number of photons \enden  \ja 発射するフォトンの目標数 \endja </xsd:documentation>
											</xsd:annotation>
										</xsd:element>
										<xsd:element name="pixel_ratio">
											<xsd:annotation>
												<xsd:documentation> \en pixel ratio \enden  \ja ピクセル縦横比 \endja </xsd:documentation>
											</xsd:annotation>
										</xsd:element>
