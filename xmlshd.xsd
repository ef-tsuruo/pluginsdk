												<xsd:documentation> \en renderers \enden  \ja プラグインレンダラが設定したカスタム属性データの配列 \endja </xsd:documentation>
											</xsd:annotation>
											<xsd:complexType>
												<xsd:choice minOccurs="0" maxOccurs="unbounded">
													<xsd:element name="face_index">
														<xsd:annotation>
															<xsd:documentation> \en void \enden  \ja 無効 \endja </xsd:documentation>
														</xsd:annotation>
													</xsd:element>
													<xsd:element name="vertex_index">
														<xsd:annotation>
															<xsd:documentation> \en void \enden  \ja 無効 \endja </xsd:documentation>
