												<xsd:documentation> \en blah \enden  \ja 1 : αブレンド \endja </xsd:documentation>
												<xsd:documentation> \en blah \enden  \ja 2 : 加算 \endja </xsd:documentation>
												<xsd:documentation> \en blah \enden  \ja 3 : 減算 \endja </xsd:documentation>
												<xsd:documentation> \en blah \enden  \ja 4 : 乗算 \endja </xsd:documentation>
												<xsd:documentation> \en blah \enden  \ja 5 : 比較（明） \endja </xsd:documentation>
												<xsd:documentation> \en blah \enden  \ja 6 : 比較（暗） \endja </xsd:documentation>
											</xsd:annotation>
										</xsd:element>
										<xsd:element name="premultiplied">
											<xsd:annotation>
