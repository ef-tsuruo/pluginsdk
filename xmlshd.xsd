												<xsd:documentation> \en blah \enden  \ja 1 : 雲 \endja </xsd:documentation>
												<xsd:documentation> \en blah \enden  \ja 2 : 海 \endja </xsd:documentation>
												<xsd:documentation> \en blah \enden  \ja 3 : 大理石 \endja </xsd:documentation>
												<xsd:documentation> \en blah \enden  \ja 4 : スポット \endja </xsd:documentation>
												<xsd:documentation> \en blah \enden  \ja 5 : チェック \endja </xsd:documentation>
												<xsd:documentation> \en blah \enden  \ja 6 : 霧 \endja </xsd:documentation>
												<xsd:documentation> \en blah \enden  \ja 7 : イメージ \endja </xsd:documentation>
