												<xsd:documentation> \en blah \enden  \ja 1 : Latitude-longitude \endja </xsd:documentation>
												<xsd:documentation> \en blah \enden  \ja 2 : Light probe \endja </xsd:documentation>
												<xsd:documentation> \en blah \enden  \ja 3 : Cube map \endja </xsd:documentation>
												<xsd:documentation> \en blah \enden  \ja 4 : Vertical cross \endja </xsd:documentation>
											</xsd:annotation>
										</xsd:element>
										<xsd:element name="projection_direction">
											<xsd:annotation>
												<xsd:documentation> \en projection direction \enden  \ja 投影方向 \endja </xsd:documentation>
