												<xsd:documentation> \en blah \enden  \ja 2 : 2 x 2 \endja </xsd:documentation>
												<xsd:documentation> \en blah \enden  \ja 3 : 3 x 3 \endja </xsd:documentation>
												<xsd:documentation> \en blah \enden  \ja 4 : 4 x 4 \endja </xsd:documentation>
											</xsd:annotation>
										</xsd:element>
										<xsd:element name="renderer_uuid">
											<xsd:annotation>
												<xsd:documentation> \en renderer \enden  \ja レンダラ識別子 \endja </xsd:documentation>
												<xsd:documentation> \en blah \enden  \ja 先頭32ビット以外が全てゼロの場合には、先頭32ビット整数の値を以下のように解釈する。 \endja </xsd:documentation>
												<xsd:documentation> \en blah \enden  \ja 0 : レイトレーシング \endja </xsd:documentation>
												<xsd:documentation> \en blah \enden  \ja 0 : パストレーシング \endja </xsd:documentation>
												<xsd:documentation> \en blah \enden  \ja 0 : スキャンライン \endja </xsd:documentation>
												<xsd:documentation> \en blah \enden  \ja それ以外の場合には、プラグインレンダラの識別子 \endja </xsd:documentation>
											</xsd:annotation>
										</xsd:element>
										<xsd:element name="shadow_map_size">
