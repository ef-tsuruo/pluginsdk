							<xsd:element ref="shape_specifier">
								<xsd:annotation>
									<xsd:documentation> \en shape_specifier \enden  \ja この形状に視点を連動させる \endja </xsd:documentation>
								</xsd:annotation>
							</xsd:element>
						</xsd:choice>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="target_link">
					<xsd:annotation>
						<xsd:documentation> \en target_link \enden  \ja 注視点連結 \endja </xsd:documentation>
					</xsd:annotation>
					<xsd:complexType>
						<xsd:choice minOccurs="0" maxOccurs="unbounded">
							<xsd:element ref="shape_specifier">
								<xsd:annotation>
									<xsd:documentation> \en shape_specifier \enden  \ja この形状に注視点を連動させる \endja </xsd:documentation>
								</xsd:annotation>
							</xsd:element>
						</xsd:choice>
					</xsd:complexType>
				</xsd:element>
			</xsd:choice>
		</xsd:complexType>
