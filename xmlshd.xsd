										<xsd:element ref="pixels">
											<xsd:annotation>
												<xsd:documentation> \en pixels \enden  \ja イメージの実体 \endja </xsd:documentation>
											</xsd:annotation>
										</xsd:element>
									</xsd:choice>
								</xsd:complexType>
							</xsd:element>
						</xsd:choice>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="radiosity">
