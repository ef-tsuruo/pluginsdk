												<xsd:documentation> \en blah \enden  \ja 1 : X軸負方向 \endja </xsd:documentation>
												<xsd:documentation> \en blah \enden  \ja 2 : Y軸正方向 \endja </xsd:documentation>
												<xsd:documentation> \en blah \enden  \ja 3 : Y軸負方向 \endja </xsd:documentation>
												<xsd:documentation> \en blah \enden  \ja 4 : Z軸正方向 \endja </xsd:documentation>
												<xsd:documentation> \en blah \enden  \ja 5 : Z軸負方向 \endja </xsd:documentation>
											</xsd:annotation>
										</xsd:element>
										<xsd:element name="color">
											<xsd:annotation>
												<xsd:documentation> \en color \enden  \ja 色 \endja </xsd:documentation>
											</xsd:annotation>
										</xsd:element>
										<xsd:element name="area">
											<xsd:annotation>
												<xsd:documentation> \en area \enden  \ja イメージの範囲 \endja </xsd:documentation>
											</xsd:annotation>
										</xsd:element>
										<xsd:element name="flags">
