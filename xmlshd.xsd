															<xsd:documentation> \en i \enden  \ja キーフレームのインデクス値 \endja </xsd:documentation>
														</xsd:annotation>
													</xsd:element>
												</xsd:choice>
											</xsd:complexType>
										</xsd:element>
									</xsd:choice>
								</xsd:complexType>
							</xsd:element>
							<xsd:element name="keyframe">
								<xsd:complexType>
									<xsd:choice minOccurs="0" maxOccurs="unbounded">
