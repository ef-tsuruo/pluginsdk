										<xsd:element name="subframes">
											<xsd:annotation>
												<xsd:documentation> \en the number of subframes \enden  \ja サブフレーム数 \endja </xsd:documentation>
												<xsd:documentation> \en blah \enden  \ja モーションブラーを計算する際のサブフレーム分割数 \endja </xsd:documentation>
											</xsd:annotation>
										</xsd:element>
										<xsd:element name="initialize_random">
											<xsd:annotation>
												<xsd:documentation> \en initialize_random \enden  \ja initialize_random \endja </xsd:documentation>
												<xsd:documentation> \en blah \enden  \ja 未使用 \endja </xsd:documentation>
											</xsd:annotation>
										</xsd:element>
										<xsd:element name="photon_map_total_energy">
