															<xsd:documentation> \en repeat \enden  \ja 繰り返しの数 \endja </xsd:documentation>
														</xsd:annotation>
													</xsd:element>
													<xsd:element name="minframe">
														<xsd:annotation>
															<xsd:documentation> \en minframe \enden  \ja 最小フレーム値 \endja </xsd:documentation>
														</xsd:annotation>
													</xsd:element>
													<xsd:element name="maxframe">
														<xsd:annotation>
															<xsd:documentation> \en maxframe \enden  \ja 最大フレーム値 \endja </xsd:documentation>
														</xsd:annotation>
													</xsd:element>
													<xsd:element name="i">
